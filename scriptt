    print("Clicked")
end)

PlayerSection:NewSlider("Fly", "Makes you fly like superman", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.Fly = Fly
