if game.PlaceID == 2788229376 then
-- MAIN
local Main = Window:NewTab("Main")
local MainSection= Main:NewSection("Main")



Main:NewToggle("Lock", "makes you have dimag16 aim", function(state)
    if state then
        <q></q>=("Toggle On")
    else
        <q></q>=("Toggle Off")
    end
end)


